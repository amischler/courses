{
  "name": "courses",
  "version": "1.0.0",
  "description": "Nextcloud Courses App",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "build": "vite build",
    "dev": "NODE_ENV=development vite build --mode development",
    "watch": "NODE_ENV=development vite build --mode development --watch",
    "serve": "vite preview",
    "dev-server": "npm run build && node dev-server.js"
  },
  "dependencies": {
    "@nextcloud/auth": "^2.2.1",
    "@nextcloud/axios": "^2.4.0",
    "@nextcloud/initial-state": "^2.1.0",
    "@nextcloud/l10n": "^3.1.0",
    "@nextcloud/router": "^3.0.1",
    "@nextcloud/vue": "^9.0.0-alpha.5",
    "dexie": "^3.2.4",
    "pinia": "^2.1.7",
    "vue": "^3.5.13",
    "vue-material-design-icons": "^5.2.0",
    "vue-router": "^4.5.0"
  },
  "overrides": {
    "@nextcloud/vue": {
      "vue": "$vue"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "express": "^5.1.0",
    "sass": "^1.90.0",
    "vite": "^5.4.19"
